public class InsertionSort
{
 public static void main(int a[])
 {
     int key=0;
      for(int j=1;j<a.length;j++)
      {
           key=a[j];
           int i=j-1;
           while(j>=0&& a[j]>key)
           {
               a[j+1]=a[j];
               j--;
               
            }
            a[j+1]=key;
        }
    }
}
               
